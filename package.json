{
  "name": "generator-react-mrmilu",
  "description": "Yeoman generator for template project in react for MrMil√∫",
  "version": "1.0.0",
  "main": "app/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmilu/generator-react-mrmilu.git"
  },
  "files": [
    "app"
  ],
  "scripts": {
    "build": "run-s build:clean build:code",
    "build:clean": "rimraf app/index.js*",
    "build:code": "rollup -c",
    "watch": "rollup -cw",
    "create:sample": "rimraf sample/my-project && yo ./app/index.js sample/my-project",
    "lint": "eslint --ext .js src/",
    "lint:fix": "yarn lint --fix",
    "clean:root": "rm -fr node_modules",
    "ci": "yarn clean:root && yarn",
    "semantic-release": "semantic-release",
    "postversion": "git commit -am 'chore(bump): bump version [skip-ci]' && git push --follow-tags"
  },
  "dependencies": {
    "glob": "7.1.6",
    "mkdirp": "1.0.4",
    "yeoman-generator": "4.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.0",
    "@commitlint/config-conventional": "12.0.0",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/jest": "26.0.20",
    "@types/yeoman-generator": "4.11.3",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "builtin-modules": "3.2.0",
    "esbuild": "0.8.51",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.6.0",
    "jest": "26.6.3",
    "jest-chain": "1.1.5",
    "jest-extended": "0.11.5",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.39.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-esbuild": "2.6.1",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "semantic-release": "17.3.9",
    "typescript": "4.2.2",
    "yo": "3.1.1"
  }
}
